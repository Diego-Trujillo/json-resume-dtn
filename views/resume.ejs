<!doctype html>
<html lang="en">

<head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
    <meta content='width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0, shrink-to-fit=no' name='viewport' />
    
    
    <title><%= schema.resume.basics.name %> | <%= schema.resume.basics.label %></title>
    
    <!--     Fonts and icons     -->
    <link href="https://fonts.googleapis.com/css?family=Montserrat:400,700,200" rel="stylesheet" />
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/latest/css/font-awesome.min.css" />
    
    <!-- CSS -->
    <link href="/css/bootstrap.min.css" rel="stylesheet" />
    <link href="/css/light-bootstrap-dashboard.css?v=2.0.1" rel="stylesheet" />
    <link href="/css/resume.css" rel="stylesheet" />

</head>

<body>
    <div class="wrapper wrapper-full-page">
        <!-- Navbar -->
        <nav class="navbar navbar-expand-lg navbar-transparent navbar-absolute">
            <div class="container">
                <div class="navbar-wrapper">
                    <a class="navbar-brand" href="#navbar"></a>
                    <button class="navbar-toggler navbar-toggler-right" type="button" data-toggle="collapse" aria-controls="navigation-index" aria-expanded="false" aria-label="Toggle navigation">
                        <span class="navbar-toggler-bar burger-lines"></span>
                        <span class="navbar-toggler-bar burger-lines"></span>
                        <span class="navbar-toggler-bar burger-lines"></span>
                    </button>
                </div>
                <div class="collapse navbar-collapse justify-content-end" id="navbar">
                    <ul class="navbar-nav">
                        <li class="nav-item ">
                            <a href="mailto:<%= schema.resume.basics.email %>" class="nav-link">
                                <i class="nc-icon nc-email-83"></i> <%= schema.pageElements.nav.contact %>
                            </a>
                        </li>
                        <li class="nav-item ">
                            <a href="<%= schema.pageElements.nav.resume.url %>" class="nav-link">
                                <i class="nc-icon nc-cloud-download-93"></i> <%= schema.pageElements.nav.resume.anchor %>
                            </a>
                        </li>
                        <li class="nav-item dropdown">
                            <a class="nav-link dropdown-toggle" data-toggle="dropdown" href="#" role="button" aria-haspopup="true" aria-expanded="false"> 
                               <i class="nc-icon "></i> <%= languages.current.toUpperCase() %> 
                            </a>
                            <div class="dropdown-menu">
                                <% languages.available.forEach(function(language){ %> 
                                  <a class="dropdown-item" href="/resume/<%= language %>">
                                    <img src="/img/flags/<%= language %>.png" />  <%= language.toUpperCase() %>
                                  </a>
                                <% }) %>
                            </div>
                        </li>
                    </ul>
                </div>
            </div>
        </nav>
        <!-- Navbar -->
        <div class="full-page" data-color="azure" data-image="/img/Background.jpg">
            <div class="content" style="padding-top:6em">
                <div class="container">
                  <div class="row">
                  <!-- Profile -->
                      <div class="col-xs-12 col-sm-12 col-md-12 col-lg-4">
                        <div class="card card-user">
                          
                            <div class="card-header">
                                <div class="author">
                                    <img class="avatar mb-2" src="<%= schema.resume.basics.picture %>" alt="..."> 
                                </div>
                            </div>
                            
                            <div class="card-body text-center mt-0">
                              <h4 class="card-title"><%= schema.resume.basics.name %></h4>
                                
                              <h5 class="card-description mt-0">
                                  <small><%= schema.resume.basics.label %></small>
                              </h5>
                              
                              <p class="card-description text-center">
                                <br />
                                  <em><%= schema.resume.basics.summary %></em>
                              </p>
                              
                            </div>
                            
                            <div class="card-footer ">
                                <hr>
                                <div class="button-container text-center">
                                    <% schema.resume.basics.profiles.forEach(function(profile){ %>
                                      <a href="<%= profile.url %>" class="btn btn-simple btn-link btn-icon" alt="@<%= profile.network %>: <%= profile.username %>">
                                        <i class="fa fa-<%= profile.network.toLowerCase() %>"></i>
                                      </a>
                                    <% }); %>
                                </div>
                            </div>
                            
                        </div>
                    </div>
                  <!-- Profile -->
                  <!-- Stuff -->
                    <div class="col-xs-12 col-sm-12 col-md-12 col-lg-8">
                      <div class="card">
                          <div class="card-body content-full-width">
                              <ul role="tablist" class="nav nav-tabs">
                                  <li role="presentation" class="nav-item show active">
                                      <a class="nav-link" id="experience-tab" href="#experience-section" data-toggle="tab">
                                        <i class="fa fa-rocket"></i> 
                                        <%= schema.pageElements.resume.experience %>
                                      </a>
                                  </li>
                                  <li class="nav-item">
                                      <a class="nav-link" id="skills-tab" href="#skills-section" data-toggle="tab">
                                        <i class="fa fa-code"></i> 
                                        <%= schema.pageElements.resume.skills %>
                                      </a>
                                  </li>
                                  <li class="nav-item">
                                      <a class="nav-link" id="portfolio-tab" href="#portfolio-section" data-toggle="tab">
                                        <i class="fa fa-briefcase"></i> 
                                        <%= schema.pageElements.resume.portfolio %>
                                      </a>
                                  </li>
                                  <li class="nav-item">
                                      <a class="nav-link" id="misc-tab" href="#misc-section" data-toggle="tab">
                                        <i class="fa fa-user"></i> 
                                        <%= schema.pageElements.resume.misc %>
                                      </a>
                                  </li>
                              </ul>
                              <div class="tab-content">
                                  <div id="experience-section" class="tab-pane fade show active pl-4" role="tabpanel" aria-labelledby="experience-tab">
                                    <!-- ------ JOB EXPERIENCE ------ -->
                                      <h3 class="mt-1 mb-3"><small>EXPERIENCIA LABORAL</small></h3>
                                      <div class="ml-2">
                                        <% schema.resume.work.forEach(function(job){ %> 
                                          <hr />
                                          <a href="<%= job.website %>">
                                            <h5 class="text-nolink my-1"><%= job.company %></h5>
                                          </a>
                                          <p class="text-position my-1">
                                            <%= job.position %> - <span class="text-gray">(<%= job.startDate %> - <%= job.endDate %>)</span>
                                          </p>
                                          
                                          <p class="text-details my-2"> 
                                            <%= job.summary %> 
                                          </p>
                                          
                                          <ul>
                                            <% job.highlights.forEach(function(highlight){ %> 
                                              <li class="text-details">
                                                <%= highlight %>
                                              </li>
                                            <% }); %>
                                          </ul>
                                          
                                          
                                        <% }); %>
                                      </div>
                                      <hr size="50" >
                                      <!-- ------ JOB EXPERIENCE ------ -->
                                      <!-- ------ SCHOOL EXPERIENCE ------ -->
                                      <h3 class="mt-2 mb-3"><small>EXPERIENCIA ESCOLAR</small></h3>
                                      <div class="ml-2">
                                        <% schema.resume.education.forEach(function(edu){ %> 
                                          <hr />
                                          <h5 class="text-dark my-1"><%= edu.institution %></h5>
                                          
                                          <p class="text-position my-1">
                                            <%= edu.area %> - <span class="text-gray">(<%= edu.startDate %> - <%= edu.endDate %>)</span>
                                          </p>
                                          
                                          <h6>CURSOS</h6>
                                          <ul>
                                            <% edu.courses.forEach(function(course){ %> 
                                              <li class="text-details">
                                                <%= course %>
                                              </li>
                                            <% }); %>
                                          </ul>
                                          
                                        
                                        <% }); %>
                                        <hr />
                                      </div>
                                      <!-- ------ SCHOOL EXPERIENCE ------ -->
                                  </div>
                                  <div id="skills-section" class="tab-pane fade" role="tabpanel" aria-labelledby="skills-tab">
                                      Aquí habilidades
                                  </div>
                                  <div id="portfolio-section" class="tab-pane fade" role="tabpanel" aria-labelledby="portfolio-tab">
                                      Aquí portafolio
                                  </div>
                                  <div id="misc-section" class="tab-pane fade" role="tabpanel" aria-labelledby="misc-tab">
                                  coasas en geral                                      
                                  </div>
                              </div>
                          </div>
                      </div>
                  </div>
                  <!-- Stuff -->
                  </div>          
                                      
                </div>
            </div>
        </div>
    </div>
</body>
<!--   Core JS Files   -->
<script src="/js/core/jquery.3.2.1.min.js" type="text/javascript"></script>
<script src="/js/core/popper.min.js" type="text/javascript"></script>
<script src="/js/core/bootstrap.min.js" type="text/javascript"></script>
<!--  Plugin for Switches, full documentation here: http://www.jque.re/plugins/version3/bootstrap.switch/ -->
<script src="/js/plugins/bootstrap-switch.js"></script>
<!--  Google Maps Plugin    -->
<script type="text/javascript" src="https://maps.googleapis.com/maps/api/js?YOUR_KEY_HERE"></script>

<!--  jVector Map  -->
<script src="/js/plugins/jquery-jvectormap.js" type="text/javascript"></script>

<!--  Sweet Alert  -->
<script src="/js/plugins/sweetalert2.min.js" type="text/javascript"></script>
<!--  Tags Input  -->
<script src="/js/plugins/bootstrap-tagsinput.js" type="text/javascript"></script>
<!--  Sliders  -->
<script src="/js/plugins/nouislider.js" type="text/javascript"></script>

<!-- Control Center for Now Ui Dashboard: parallax effects, scripts for the example pages etc -->
<script src="/js/light-bootstrap-dashboard.js?v=2.0.1" type="text/javascript"></script>

<script>
  $(document).ready(function(){
    page = $('.full-page');
    image_src = page.data('image');

    if (image_src !== undefined) {
      image_container = '<div class="full-page-background" style="background-image: url(' + image_src + ') "/>'
      page.append(image_container);
    }
  });
</script>

</html>